        }
    }
}

patsient1 = new Patsient("Thomas", 32)
patsient2 = new Patsient("Ranno", 33)
arst1 = new Arst("Babushka Marina", 31)
arst2 = new Arst("John Pickle", 36)
haigla1 = new Haigla()
haigla1.patsiendiList.push(patsient1)
haigla1.patsiendiList.push(patsient2)
haigla1.showAllPatsiendid();
haigla1.arstideList.push(arst1)
haigla1.arstideList.push(arst2)
haigla1.showAllArstid();
haigla1.showArstiAeg();
