class Patsient{
    constructor (nimi, vanus){
        this.nimi = nimi
        this.vanus = vanus
    }
}

class Arst{
    constructor(nimi, vanus){
        this.nimi = nimi
        this.vanus = vanus
    }
}

class Haigla{
    constructor(){
        this.patsiendiList = []
        this.arstideList = []
    }

    showAllPatsiendid(){
        for(let i = 0; i < this.patsiendiList.length; i++){
            console.log("Patsiendi nimi: ", this.patsiendiList[1].nimi)
        }
    }
    showAllArstid(){
        for(let t = 0; t < this.arstideList.lenght; t++){
            console.log("Arsti nimi; ", this.arstideList[t].nimi)
        }
    }
    
    showArstiAeg(){
        let arstiNimi
        arstiNimi = prompt("Milline arst teid huvitab? : ");
