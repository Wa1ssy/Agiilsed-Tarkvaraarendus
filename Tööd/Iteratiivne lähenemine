            if(this.arstideList[t].nimi == arstiNimi){
                for(let j = 0; j < this.arstideList[t].aeg.lenght; j++){
                    console.log(this.arstideList[t].nimi) 
                }
            }
            arstiAeg = prompt("Milline aeg sobib: ");
            kohtumine1 = new Kohtumine(PatsiendiNimi.ArstiNimi.ArstiNimi.ArstiAeg)
            this.kohtumisList.psuh(kohtumine1)
        }
    }
    
